{
  "name": "@stefano.amorelli/opentelemetry-instrumentation-dust",
  "version": "0.4.0",
  "description": "OpenTelemetry instrumentation for Dust API following gen-ai-agent-spans semantic conventions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "manual-test": "bun run manual-test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "opentelemetry",
    "instrumentation",
    "dust",
    "gen-ai",
    "tracing",
    "observability"
  ],
  "author": {
    "name": "Stefano Amorelli",
    "email": "stefano@amorelli.tech",
    "url": "https://amorelli.tech"
  },
  "license": "GPL-3.0",
  "peerDependencies": {
    "@dust-tt/client": "^1.1.0",
    "@opentelemetry/api": "^1.9.0"
  },
  "devDependencies": {
    "@dust-tt/client": "^1.1.18",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "@opentelemetry/sdk-trace-node": "^1.28.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "^0.56.0",
    "@opentelemetry/semantic-conventions": "^1.28.0"
  },
  "resolutions": {
    "eventsource-parser": "^1.1.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanoamorelli/opentelemetry-instrumentation-dust.git"
  },
  "homepage": "https://www.npmjs.com/package/@stefano.amorelli/opentelemetry-instrumentation-dust"
}
